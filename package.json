{
  "name": "ssr2",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:dev:client": "webpack-dev-server --mode development --config webpack.dev.client.js",
    "build:dev:server": "webpack --mode development --config webpack.dev.server.js",
    "start:dev": "nodemon --watch bin bin/server.js",
    "start": "run-p build:dev:client build:dev:server",
    "build:prod:client": "webpack --mode production --config webpack.prod.client.js",
    "build:prod:server": "webpack --mode production --config webpack.prod.server.js",
    "build:prod": "webpack --mode production --config webpack.prod.config.js",
    "start:prod": "node bin/server.js",
    "start:prod:all": "run-s build:prod start:prod",
    "prettier": "prettier",
    "lint:fix": "eslint --fix .",
    "prettier:style": "prettier-stylelint --write 'src/**/*.{css,scss}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier-stylelint --write",
      "git add"
    ]
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.10.2",
    "@loadable/component": "^5.10.3",
    "@loadable/server": "^5.10.3",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "connected-react-router": "^6.5.2",
    "express": "^4.17.1",
    "http": "^0.0.0",
    "prop-types": "^15.7.2",
    "puppeteer": "^2.0.0",
    "react": "^16.10.2",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.1",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "serialize-javascript": "^2.1.0",
    "universal-cookie": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@loadable/babel-plugin": "^5.10.3",
    "@loadable/webpack-plugin": "^5.7.1",
    "autoprefixer": "^9.6.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "prettier-stylelint": "^0.4.2",
    "react-hot-loader": "^4.12.15",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.1",
    "start-server-webpack-plugin": "^2.2.5",
    "stylelint-prettier": "^1.1.1",
    "terser-webpack-plugin": "^2.1.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^4.0.0"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ]
}
